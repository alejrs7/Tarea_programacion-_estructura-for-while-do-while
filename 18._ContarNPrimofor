#include <stdio.h>

int esPrimo(int num) {
    if (num <= 1) return 0;
    for (int i = 2; i <= num / 2; i++) {
        if (num % i == 0) return 0;
    }
    return 1;
}

int main() {
    int n, count = 0;

    printf("Introduce un número: ");
    scanf("%d", &n);

    for (int i = 2; i <= n; i++) {
        if (esPrimo(i)) count++;
    }

    printf("Hay %d números primos hasta %d\n", count, n);
    return 0;
}
